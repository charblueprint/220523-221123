
public class Scan5 {

	public static void main(String[] args) {
		byte A = 10;
		
		if(A < 10) {
			System.out.println("10이상 숫자를 입력하세요.");
		}
		else if((A >= 10) || (A <= 22)) { // 조건문에 범위를 설정하여 적용함
			System.out.println("당첨입니다.");
		}
		else {
			System.out.println("범위 밖의 숫자 입니다.");
		}
	
	
	
	
	
	
	
	
	
	
	}
	

}
