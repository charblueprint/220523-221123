package Questions0525;

public class Q1 {

	public static void main(String[] args) {
		//응용문제#2(0525) 
		//Q: 20~27 출력
		System.out.println("1번 출력결과 :");
		int A;
		for(A=20; A<=27; A++) {
			System.out.print(A+" ");
		}
		
		System.out.println();
		
		/*응용문제 #3
		 * Q 39~21 출력
		 */
		System.out.println("2번 출력결과 :");
		int B;
		for(B=39; B>=21; --B) {
			System.out.print(B+" ");
		}
		
	}

}
