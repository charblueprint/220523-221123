package Classtime;

public class For1 {

	public static void main(String[] args) {
		//반복문 : 데이터를 일괄적으로 입력, 출력을 할 수 있도록 하는 문법 - 게시판, 메일등지에 쓰임
		int z = 1;
		//z++; : 출력 후 +1
		//z--; : 출력 후 -1
		//++z; : +1 후 출력 
		//--z; : -1 후 출력
		System.out.println(z);
		//for(초기값; 범위값; 증감여부)
		for(int a=0; a<10; a++) {
			System.out.print(a);
			System.out.print(" ");
		}
		
		int b;
		for(b=5; b<=10; b++) {
			System.out.println(b);
		}
		
	}

}
